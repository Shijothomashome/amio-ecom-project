    <!-- Login success alert -->
    <% if (locals.loginSuccess) { %>
        <div id="slideInAlert" class="alert alert-success slide-in">
          <%= loginSuccess %>
        </div>
    
      <style>
        #slideInAlert {
      display: none;
      position: fixed;
      top: 15%;
      left: 50%;
      transform: translate(-50%, -50%);
      padding: 10px 30px;
      border-radius: 10px;
      background-color: #28a745;
      color: #fff;
      z-index: 1000;
    }
    .slide-in {
      animation: slideIn 0.5s ease-in-out;
    }
    
    @keyframes slideIn {
      from {
        transform: translate(-50%, -100%);
      }
      to {
        transform: translate(-50%, -50%);
      }
    }
      </style>
      <script>
        document.addEventListener('DOMContentLoaded', () => {
      const slideInAlert = document.getElementById('slideInAlert');
    
      if (slideInAlert) {
        // Display the alert
        slideInAlert.style.display = 'block';
    
        // Hide the alert after 3 seconds
        setTimeout(() => {
          slideInAlert.style.display = 'none';
        }, 3000);
      }
    });
      </script>
      <% } %>
    <!-- End of Login success alert -->


